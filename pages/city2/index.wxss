/**index.wxss**/
.container {
    font-size: 0.9em;
    padding: 24rpx;
    background: #fff;
}
.show {
    display: block;
}
.hidden {
    display: none;
}
.return {
    float: right;
    /*color: #6f95fb;*/
    color: green;
    border:1px solid #ddd;
    padding:10rpx 20rpx;
    margin-top:-10rpx;
    border-radius: 2px;
}
.icon-sel {
    color: #ddd;
    margin-left: 4rpx;
}
.icon-sel-txt {
    color: green;
}
.txt {
    /*font-size: 0.9em;*/
    padding-bottom: 15rpx;
    color: #666666;
    float:left;
    width:100%;
}
.littip {
    color: #999;
    font-size: 0.8em;
    padding-bottom: 25rpx;
}
.areas-box {
    border: 1px solid #ddd;
    width: 100%;
    border-radius: 2px;
}

.areas-column {
    width: 33%;
    float: left;
}

.areas-city {
    border-right: 1px solid #eee;
}
.areas-item-prov {
    height: 50px;
    line-height: 50px;
    /*background-color: #ddd;*/
    padding-left: 20rpx;
}
.areas-item-city {
    height: 50px;
    line-height: 50px;
    padding-left: 20rpx;
}
.areas-item-prov-select {
    /*color: #3573fa;*/
    color: green;
    background-color: #fff;
}
.areas-item-select {
    color: green;
}
